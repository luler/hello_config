[program:test]
command= XXX
;process_name=%(process_num)03d   ;当设置numprocs大于1时，进程名设为自动补全三位数字的进程数，如000，001，002，003，004，005
;numprocs=1                    ; number of processes copies to start (def 1)
directory=/home/wwwroot/api/XXX                ; directory to cwd to before exec (def no cwd)
priority=1                  ; the relative start priority (default 999)
autostart=true                ; start at supervisord start (default: true)
autorestart=true              ; retstart at unexpected quit (default: true)
startsecs=10                  ; number of secs prog must stay running (def. 1)
startretries=3                ; max # of serial start failures (default 3)
user=root                   ; setuid to this UNIX account to run the program
redirect_stderr=true          ; redirect proc stderr to stdout (default false)
stdout_logfile=./supervisor.log        ; stdout log path, NONE for none; default AUTO
stdout_logfile_maxbytes=1MB   ; max # logfile bytes b4 rotation (default 50MB)
stdout_logfile_backups=10     ; # of stdout logfile backups (default 10)
stdout_capture_maxbytes=1MB   ; number of bytes in 'capturemode' (default 0)
